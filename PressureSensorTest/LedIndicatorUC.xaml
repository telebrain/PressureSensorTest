<UserControl x:Class="PressureSensorTest.LedIndicatorUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PressureSensorTest"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="50">
    <Grid>
        <Rectangle Stroke="Gray" RadiusX="3" RadiusY="3"></Rectangle>
        <Rectangle x:Name="Led" Margin="3" Fill="Green" RadiusX="3" RadiusY="3"></Rectangle>    
    </Grid>
</UserControl>
